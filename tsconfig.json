{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@stores/*": ["src/stores/*"],
      "@constants/*": ["src/constants/*"],
      "@lib/*": ["src/lib/*"],
      "@components/*": ["src/components/*"]
    },
    "ignoreDeprecations": "6.0"
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
